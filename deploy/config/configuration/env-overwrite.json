                },
                {
                    "Key": "NamedEndpoints/IdentityService/Port",
                    "Value": "80"
                },
                {
                    "Key": "NamedEndpoints/IdentityService-External/Address",
                    "Value": "identity. {{- .Values.environment.domain }}"
