                    "Key": "Connections/Sql/MaverickQuery",
                    "Value": "{{"{{"}}$this/Properties/ConnectionString}} Initial Catalog={{"{{"}}$this/Properties/Prefix}}{{.Values.sql.databases.core.query}};"
                },
